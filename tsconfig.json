{
    "compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"jsx": "react",
		"lib": ["dom", "es2019"],
		"moduleResolution": "node",
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"sourceMap": true
	}
}
